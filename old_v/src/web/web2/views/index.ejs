<!-- views/index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Dashboard</title>
</head>
<body>
  <h1>Project Dashboard</h1>
  <ul>
    <% projects.forEach(project => { %>
      <li><%= project.name %> - <%= project.description %></li>
    <% }); %>
  </ul>
  <button onclick="location.href='/createProject'">Create Project</button>
</body>
</html>
